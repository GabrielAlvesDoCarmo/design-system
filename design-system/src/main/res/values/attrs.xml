<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LogoView">
        <attr name="logoText" format="string" />
        <attr name="logoImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="DSEditText">
        <attr name="android:required" format="boolean"/>
        <attr name="validationType" format="enum">
            <enum name="none" value="0" />
            <enum name="phone" value="1" />
            <enum name="cep" value="2" />
            <enum name="description" value="3" />
            <enum name="password" value="4" />
            <enum name="email" value="5" />
            <enum name="date" value="6" />
            <enum name="time" value="7" />
        </attr>
    </declare-styleable>
</resources>